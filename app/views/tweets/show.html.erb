 <h1>Tweet</h1>

	<h3> <strong><%=@tweet.title %></strong></h3>
 <p>
 	<%= @tweet.message  %>
 </p>
 <%=link_to "Crear comentario", new_tweet_comment_path(@tweet)%>
 <ul>
    <% @comments.each do |comment|%>
        <li>
            <%=comment.content%>
        </li>
    <%end%>
</ul>

 <%= link_to 'Edit', edit_tweet_path(@tweet) %> |
<%= link_to 'Back', tweets_path %>
